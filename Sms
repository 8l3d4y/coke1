local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("EsHub - Strongman Simulator ", "Ocean")

local MainTab = Window:NewTab("Main")
local MainSection = MainTab:NewSection("Main")

MainSection:NewToggle("Auto Get strength", "Automatically Gets strength", function(v)
        getgenv().autoswing = v
        while true do
            if not getgenv().autoswing then return end
            wait(0)
local args = {
    [1] = 2,
    [2] = "Default"
}

game:GetService("ReplicatedStorage"):WaitForChild("StrongMan_UpgradeStrength"):InvokeServer(unpack(args))

        end
    end)
local PetsTab = Window:NewTab("Pets")
local PetsSection = PetsTab:NewSection("Pets")

PetsSection:NewButton("lobbyPet", "pets", function()
    local args = {
    [1] = "LobbyShop"
}

game:GetService("ReplicatedStorage"):WaitForChild("TGSPetShopRoll"):InvokeServer(unpack(args))
end)

